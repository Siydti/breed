page {
	height: 100%;
	width: 100%;
}
.schedule {
	height: 100%;
	width: 100%;
	background-color: #EBEBEB;
	padding-top: 1rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
}
.schedule_msg {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	padding: 32rpx 40rpx;
	color: #333333;
	font-size: 30rpx;
	background-color: #fff;
	margin-bottom: 1px;
}
.schedule_list {
	background-color: #FFFFFF;
	padding-top: 15rpx;
}
.schedule_list_box {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	padding: 0px 26rpx;
	line-height: 55rpx;
}
.schedule_list_left {
	color: #333333;
	font-size: 26rpx;
	font-weight: bold;
}
.schedule_list_right {
	color: #333333;
	font-size: 26rpx;
}
.schedule_flow{
	height: 200rpx;
	width: 100%;
	background-color: #fff;
	padding-top: 20rpx;
	margin-top: 2rpx;
}
.van_steps_box{
	width: 90%;
	margin: 30rpx auto;
}
.lookmore{
	color: #04C272;
	font-size: 30rpx;
	line-height: 88rpx;
	margin-top: 2rpx;
	text-align: center;
	background-color: #fff;
	margin-bottom: 20rpx;
}
.schedule_dispose{
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	        flex: 1;
	background-color: #fff;
	padding-left: 40rpx;
}
.schedule_dispose-title{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	height: 92rpx;
	color: #333333;
	font-size: 30rpx;
	font-weight: bold;
	border-bottom: 1rpx solid #EBEBEB;
}
.schedule_dispose_content{
	padding-top: 40rpx;
}
.schedule_dispose_list{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	margin-bottom: 40rpx;
}
.schedule_dispose_list_right_top{
	font-size: 26rpx;
	color: #333333;
	margin-bottom: 12rpx;
}
.schedule_dispose_list_right_bottom{
	font-size: 24rpx;
	color: #999;
}

