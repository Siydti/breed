page{
	height: 100%;
	width: 100%;
}
.setSite{
	width: 100%;
	height: 100%;
	background-color: #f2f2f2;
}
.setSite_main{
	background-color: #fff;
	padding-left: 20rpx;
}
.setSite_consignee,.setSite_phone,.setSite_site{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	height: 110rpx;
	border-bottom: 1rpx solid #E8E8E8;
}
.setSite_left{
	text-indent: 6rpx;
	color: #666666;
	font-size: 30rpx;
	width: 210rpx;
}
.setSite_right{
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	        flex: 1;
	color: #AAAAAA;
	font-size: 30rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
}
.setSite_text{
	width: 100%;
	height: 100rpx;
	color: #AAAAAA;
	font-size: 30rpx;
	padding-left: 26rpx;
	padding-top: 38rpx;
}
.setDefault{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	margin-top:52rpx;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.setDefault_left{
	color: #666666;
	font-size: 28rpx;
	margin-left: 26rpx;
}
.setDefault_right{
	margin-right: 30rpx;
}
.btn_box{
	width: 668rpx;
	height: 75rpx;
	border-radius: 75rpx;
	color: #fff;
	line-height: 75rpx;
	text-align: center;
	font-size: 34rpx;
	background-color: #FF6C00;
	position: fixed;
	bottom: 110rpx;
	left: 50%;
	-webkit-transform: translateX(-50%);
	        transform: translateX(-50%);
}

