<view class="transactionRecord"><view class="transactionRecord_title"><block wx:for="{{title}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['switchover',[index]]]]]}}" class="{{[(index==active)?'transactionRecord_title_active':'']}}" bindtap="__e">{{''+item.title+''}}</view></block></view><view hidden="{{!(active!=4)}}" class="transactionRecord_list"><block wx:for="{{payDetail_list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="payDetail_list"><view class="payDetail_list_left"><view class="payDetail_list_left_money">{{''+(item.type=="充值"?'+':'-')+item.money+''}}</view><view class="payDetail_list_left_type">{{''+item.type+''}}</view></view><view class="payDetail_list_right"><view class="payDetail_list_right_ucceed">{{''+item.ucceed+''}}</view><view class="payDetail_list_right_time">{{''+item.time+''}}</view></view></view></block></view><view hidden="{{!(active==4)}}" class="qiun-columns"><view class="qiun-charts"><canvas class="charts" canvas-id="canvasLineA" id="canvasLineA" data-event-opts="{{[['touchstart',[['touchLineA',['$event']]]]]}}" bindtouchstart="__e"></canvas></view></view></view>