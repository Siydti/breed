page{
	width: 100%;
	height: 100%;
}
.growthDynamic{
	width: 100%;
	height: 100%;
}
.title_box{
	position: fixed;
	top: 200rpx;
	left: 50%;
	-webkit-transform: translateX(-50%);
	        transform: translateX(-50%);
	width: 486rpx;
	height: 83rpx;
}
.img_title{
	width: 100%;
	height: 100%;
}
.title{
	position: absolute;
	height: 100%;
	width: 100%;
	top: 0;left: 0;
	text-align: center;
	color: #fff;
	font-size: 32rpx;
	line-height: 76rpx;
}
.main_box{
	width: 683rpx;
	height: 590rpx;
	position: fixed;
	top: 300rpx;
	left: 50%;
	-webkit-transform: translateX(-50%);
	        transform: translateX(-50%);
}
.img_main{
	width: 100%;
	height: 100%;
}
.main_listBox{
	width: 90%;
	position: absolute;
	top: 0;left: 0;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
	padding: 0px 30rpx;
}
.main_list,.main_listEnd{
	width: 100%;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	border-bottom: 1rpx solid #d9d9d9;
	padding: 40rpx 0px;
}
.main_list_left,.main_list_left{
	width: 202rpx;
	text-indent: 25rpx;
	color: #333333;
	font-size: 36rpx;
}
.main_list_mid,.main_list_mid{
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	        flex: 1;
	background-color: #F2F2F2;
	border: 1rpx solid #EAEAEA;
	height: 60rpx;
	line-height: 60rpx;
	text-align: right;
	font-size: 24rpx;
	color: #999999;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: end;
	-webkit-justify-content: flex-end;
	        justify-content: flex-end;
	padding: 0px 10rpx ;
}
.main_list_mid>view{
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	        flex: 1;
	color: #333333;
	font-size: 32rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
}
.main_list_right,.main_list_right{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: start;
	-webkit-justify-content: flex-start;
	        justify-content: flex-start;
	padding-left: 14rpx;
	width: 140rpx;
	-webkit-box-align: start;
	-webkit-align-items: flex-start;
	        align-items: flex-start;
}
.right_top{
	color: #FF6C00;
	font-size: 20rpx;
}
.main_listEnd{
	border: none;
}

