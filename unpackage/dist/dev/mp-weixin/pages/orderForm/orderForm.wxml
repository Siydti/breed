<view class="orderForm"><block wx:for="{{orderForm_list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="orderForm_list"><view class="orderForm_listTitle"><image style="width:32rpx;height:32rpx;margin-right:20rpx;margin-left:10rpx;border-radius:12rpx;" src="/static/img/ClaimMsg_log.png"></image><text>{{item.title}}</text></view><view class="orderForm_listMain"><view class="orderForm_listMain_left"><image style="width:100%;height:100%;" src="{{item.imgurl}}"></image></view><view class="orderForm_listMain_mid"><view>编号：<text>{{item.num}}</text></view><view>性别：<text>{{item.sex}}</text></view><view>体重：<text>{{item.weight}}</text></view><view>认领时间：<text>{{item.time}}</text></view></view><view class="orderForm_listMain_right">x<text>{{item.checkedNum}}</text></view></view><view class="orderForm_listFoot"><view class="orderForm_listFoot_left"><text>出售价格</text><image style="width:28rpx;height:28rpx;margin-left:4rpx;" src="/static/img/modification.png"></image></view><view class="orderForm_listFoot_right">￥<text>{{item.price}}</text></view></view></view></block><view data-event-opts="{{[['tap',[['go_tradingParticulars',['$event']]]]]}}" class="orderForm_btn" bindtap="__e">提交</view></view>